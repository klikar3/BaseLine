{
    "name": "klikar3/BaseLine",
    "description": "SQL Monitoring Tool",
    "keywords": ["yii2", "Baseline", "klikar3"],
    "homepage": "https://github.com/klikar3/BaseLine/",
    "type": "project",
    "license": "GNU General Public License",
    "support": {
        "issues": "https://github.com/klikar3/BaseLine/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/klikar3/BaseLine"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.5",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-dropdown-x": "*",
		    "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-mpdf": "@dev",
		    "kartik-v/yii2-tabs-x": "*",
        "kartik-v/yii2-krajee-base": "*",
        "klikar3/rgraph": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800,
        "github-protocols": ["https"]
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
